	<!-- ========================================================================
					Quantitative: Update Rating Definition
	  ======================================================================== -->
	<xs:complexType name="ComplexUpdateRatingType">
		<xs:sequence>
			<xs:element name="ReportingType" type="dtypes:ReportingTypeType" fixed="CHG" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ReportingReason" type="dtypes:StringRestricted300Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ChangeReason" type="dtypes:ChangeReasonType" fixed="U" minOccurs="1" maxOccurs="1"/>
			<xs:element name="UpdateDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="RatingIdentifier" type="dtypes:StringRestricted40Type" minOccurs="1" maxOccurs="1"/>
