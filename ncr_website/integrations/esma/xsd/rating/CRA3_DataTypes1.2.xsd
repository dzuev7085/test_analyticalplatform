			<xs:documentation>"Error message" : Reference error code corresponding to the rating data report error reference. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="EQU-[0-9][0-9][0-9]"/>
		</xs:restriction>
