	<!-- ========================================================================
					Qualitative Data - CRA and methodology description 
	  ======================================================================== -->
	<xs:simpleType name="ReportingTypeType">
		<xs:annotation>
			<xs:documentation>"Reporting type" : Establishes if the reporting represents new information, change (update/correction) or deletion.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="3"/>
			<xs:enumeration value="NEW"/>
			<xs:enumeration value="CHG"/>
			<xs:enumeration value="DEL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RestrictiveReportingTypeType">
		<xs:annotation>
			<xs:documentation>"Reporting type" : Establishes if the reporting represents new information or change (update/correction).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="3"/>
			<xs:enumeration value="NEW"/>
			<xs:enumeration value="CHG"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChangeReasonType">
		<xs:annotation>
			<xs:documentation>"Change reason" : The change to reported data can be due to incorrect data (correction) or due to real changes in time of the information (updates). The correction will replace the piece of information while the update is available only from the 'Update date' (the previous value being kept in the historical data).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="1"/>
			<xs:enumeration value="U"/>
			<xs:enumeration value="C"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ComplexReportingTypeInfoType">
		<xs:sequence>
			<xs:element name="ReportingType" type="dtypes:RestrictiveReportingTypeType"/>
			<xs:element name="ChangeReason" type="dtypes:ChangeReasonType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="ReportingReason" type="dtypes:StringRestricted300Type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="UpdateDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComplexCRAInfoType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"CRA Info" : Informations on the CRA.</xs:documentation>
			</xs:annotation>
			<xs:element name="ReportingTypeInfo" type="dtypes:ComplexReportingTypeInfoType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CRAName" type="dtypes:StringRestricted90Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CRADescription" type="dtypes:StringRestricted500Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CRAMethodology" type="dtypes:StringRestricted4000Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CRAMethodologyWebPageLink" type="dtypes:StringRestricted300Type" minOccurs="1"/>
			<xs:element name="SolicitedUnsolicitedRatingPolicyDescription" type="dtypes:StringRestricted500Type" minOccurs="0"/>
			<xs:element name="SubsidiaryRatingPolicy" type="dtypes:StringRestricted500Type" minOccurs="0" maxOccurs="1"/>
			<xs:element name="GeographicalReportingScope" type="dtypes:GeographicalReportingScopeType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DefinitionDefault" type="dtypes:StringRestricted2000Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CRAWebsiteLink" type="dtypes:StringRestricted40Type" minOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GeographicalReportingScopeType">
		<xs:annotation>
			<xs:documentation>"Geographical reporting scope" : In the case of a CRA part of a group, they should mention whether they report all the ratings issued by the group (global scope) or not (only the EU and endorsed ratings). If the coverage is not global, the credit rating agency shall explain why not. For all other CRAs it should be reported as "global"</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GlobalReportingScopeFlag" type="dtypes:BooleanType"/>
			<xs:element name="ReasonNonGlobal" type="dtypes:StringRestricted500Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ========================================================================
					Qualitative Data - Issuer Debt classification
	  ======================================================================== -->
	<xs:simpleType name="SeniorityType">
		<xs:annotation>
			<xs:documentation>"Seniority Type" .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:maxLength value="3"/>
			<xs:enumeration value="SEU"/>
			<xs:enumeration value="SEO"/>
			<xs:enumeration value="SB"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ComplexDebtClassificationType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"Issuer debt classification" : Description of the issuer debt classification used by the CRA.</xs:documentation>
			</xs:annotation>
			<xs:element name="ReportingTypeInfo" type="dtypes:ComplexReportingTypeInfoType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DebtClassificationCode" type="dtypes:StringRestricted40Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DebtClassificationName" type="dtypes:StringRestricted90Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DebtClassificationDescription" type="dtypes:StringRestricted500Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Seniority" type="dtypes:SeniorityType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DebtClassificationStartDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DebtClassificationEndDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IssuerRatedDebtClassificationListType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"Classification of issuer rated Debt"</xs:documentation>
			</xs:annotation>
			<xs:element name="RatedDebtClassification" type="dtypes:ComplexDebtClassificationType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IssuerRatingListType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"IssuerRatingInfo"</xs:documentation>
			</xs:annotation>
			<xs:element name="IssuerRatingInfo" type="dtypes:ComplexIssuerRatingInfoType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IssuerRatingTypeStandardType">
		<xs:annotation>
			<xs:documentation>"Issuer Rating Type Standard Type. This should distinguish the types of issuer ratings in: the main/global issuer credit rating, the debt rating type and all other issuer debt ratings.
			IR – Main issuer rating
            DT - Debt rating
            OT - Other" .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="2"/>
			<xs:enumeration value="IR"/>
			<xs:enumeration value="DT"/>
			<xs:enumeration value="OT"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ComplexIssuerRatingInfoType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"Issuer debt classification" : Description of the issuer rating used by the CRA.</xs:documentation>
			</xs:annotation>
			<xs:element name="ReportingTypeInfo" type="dtypes:ComplexReportingTypeInfoType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeCode" type="dtypes:StringRestricted40Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeName" type="dtypes:StringRestricted90Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeDescription" type="dtypes:StringRestricted500Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeStandard" type="dtypes:IssuerRatingTypeStandardType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeStartDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssuerRatingTypeEndDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ========================================================================
					Qualitative Data - Issue/program types
	  ======================================================================== -->
	<xs:complexType name="ComplexIssueProgramType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"Issuer/program types.</xs:documentation>
			</xs:annotation>
			<xs:element name="ReportingTypeInfo" type="dtypes:ComplexReportingTypeInfoType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssueProgramCode" type="dtypes:StringRestricted40Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssueProgramName" type="dtypes:StringRestricted90Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssueProgramDescription" type="dtypes:StringRestricted500Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssueProgramStartDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IssueProgramEndDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IssueProgramListType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"List of issue/program"</xs:documentation>
			</xs:annotation>
			<xs:element name="IssueProgram" type="dtypes:ComplexIssueProgramType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ========================================================================
					Qualitative Data - Lead Analyst list
	  ======================================================================== -->
	<xs:complexType name="ComplexLeadAnalystType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"Lead analyst identification (id and name) and validity dates (valid Start and valid End)"</xs:documentation>
			</xs:annotation>
			<xs:element name="ReportingTypeInfo" type="dtypes:ComplexReportingTypeInfoType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="LeadAnalystCode" type="dtypes:StringRestricted40Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="LeadAnalystName" type="dtypes:StringRestricted90Type" minOccurs="1" maxOccurs="1"/>
			<xs:element name="LeadAnalystStartDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="LeadAnalystEndDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LeadAnalystListType">
		<xs:sequence>
			<xs:annotation>
				<xs:documentation>"List of Lead analyst identification (id and name) and validity dates (valid Start and valid End)"</xs:documentation>
			</xs:annotation>
			<xs:element name="LeadAnalyst" type="dtypes:ComplexLeadAnalystType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ========================================================================
					Qualitative Data - Rating Scale
	  ======================================================================== -->
	<xs:simpleType name="TimeHorizonType">
		<xs:annotation>
			<xs:documentation>"TimeHorizonType" Identifies the applicability of the rating scale based on the time horizon. 
			L - applicable to long term ratings
            S – applicable to short term ratings" .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="1"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="S"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RatingTypeType">
		<xs:annotation>
			<xs:documentation>"RatingTypeType": C - corporate ratings, S - Sovereign and public finance ratings, T - Structured finance ratings
O – Other financial instruments .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:AlphanumericType">
			<xs:length value="1"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="T"/>
			<xs:enumeration value="O"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RatingScaleScopeType">
		<xs:annotation>
			<xs:documentation>"PR - rating scale is used for issuing preliminary ratings only
FR - rating scale is used for issuing final ratings only
BT - rating scale is used for issuing preliminary and final ratings.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
			<xs:length value="2"/>
			<xs:enumeration value="PR"/>
			<xs:enumeration value="FR"/>
			<xs:enumeration value="BT"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ComplexScopeListType">
		<xs:annotation>
			<xs:documentation>"Scope of the rating scale". If the rating scale identifier is populated, then the scope is mandatory.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Scope" type="dtypes:ComplexScopeType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ComplexScopeType">
		<xs:annotation>
			<xs:documentation>"Scope of the rating scale". If the rating scale identifier is populated, then the scope is mandatory.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TimeHorizon" type="dtypes:TimeHorizonType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="RatingType" type="dtypes:RatingTypeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="RatingScaleScope" type="dtypes:RatingScaleScopeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="RelevantForCEREPFlag" type="dtypes:BooleanYNType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ScopeStartDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ScopeEndDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
