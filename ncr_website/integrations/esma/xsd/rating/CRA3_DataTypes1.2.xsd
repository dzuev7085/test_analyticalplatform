	<xs:simpleType name="FileNameType">
		<xs:annotation>
			<xs:documentation>"File name" : Format: 5(a)_6(a)_5(a)_6(n)_2(n).3(a)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
