<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dtypes="urn:publicid:ESMA:CRA3:DATATYPES:V1.0" targetNamespace="urn:publicid:ESMA:CRA3:DATATYPES:V1.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:include schemaLocation="StandardDataTypes1.0.xsd"/>
	<xs:include schemaLocation="CommonErrorDataTypes1.0.xsd"/>
	<xs:annotation>
		<xs:documentation>This document provides the types that the FEES system uses for DATFQL and DATFQN XML files. v1.0.
									  It extends from StandardDataTypes and CommonErrorDataTypes which are files common to all the ESMA messages.
		</xs:documentation>
	</xs:annotation>
	<!-- ========================================================================
					Global Types can be assigned to any elements with the same Type profile
	  ======================================================================== -->
		<xs:simpleType name="StringRestricted40Type">
		<xs:annotation>
			<xs:documentation>Global String Restricted to 40 digits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="40"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="AlphaNumeric5Type">
		<xs:annotation>
			<xs:documentation>Global Alpha Numeric Type Restricted to 5 digits.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:AlphanumericType">
			<xs:whiteSpace value="collapse"/>
			<xs:maxLength value="5"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
		
	<xs:simpleType name="Integer1to99Type">
		<xs:annotation>
			<xs:documentation>"Integer minimum value 1 and maximum value 99.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="UnsignedInteger15pType">
		<xs:annotation>
			<xs:documentation>Format: Max 14(n) without decimal numbers. 
				Restriction not to allow negative values </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="15"/>
			<xs:fractionDigits value="0"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="nonNegativeInteger" id="nonNegativeInteger">
		<xs:annotation>
			<xs:documentation>Positive integer, including 0. No max limit</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	
	
	<xs:simpleType name="DecimalRestrictedType">
		<xs:annotation>
			<xs:documentation>Maximum 2 decimal digitis.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">			
			<xs:fractionDigits value="2"/>			
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="IntegerRestrictedType">
		<xs:annotation>
			<xs:documentation>Only values 1,2,3</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>			
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="LanguageType">
		<xs:annotation>
			<xs:documentation>"Langage of the press release, ISO 639-1"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[a-z]+"/>
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
