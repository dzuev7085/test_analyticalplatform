	<xs:simpleType name="ModelIndicationType">
		<xs:annotation>
			<xs:documentation>"‘I’ for issuer-pays model, and/ or ‘V’ for investor-pays or ‘S’ for investor-pays or subscriber pays model" .</xs:documentation>
		</xs:annotation>
		<xs:restriction base="dtypes:UpperAlphaType">
